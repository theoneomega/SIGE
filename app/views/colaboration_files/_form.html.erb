<%= simple_form_for(@colaboration_file) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :id %>
    <%= f.input :file %>
    <%= f.input :description %>
    <%= f.input :colaboration_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
