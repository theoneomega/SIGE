<%content_for :breadcrumb do%>
<ul class="breadcrumb">
  <li><%=link_to t(:home), root_path%><span class="divider">/</span></li>
  <%if can? :index, IphPerson %>
  <li class="active"><%=link_to t(:index), iph_people_path %><span class="divider">/</span></li>
  <%end%>
  <%if can? :new, IphPerson %>
  <li class="active"><%=link_to t(:new), new_iph_person_path%></li>
  <%end%>
</ul>
<%end%>


<div class="row-fluid">
  <div class="block">
    <a href="#client-info" class="block-heading" data-toggle="collapse"><%= t(:general_information) %></a>
    <div id="client-info" class="block-body collapse in show-details">
              <p>
          <b><%= t'.id' %>:</b>
          <%= @iph_person.id %>
        </p>
              <p>
          <b><%= t'.ci' %>:</b>
          <%= @iph_person.ci %>
        </p>
              <p>
          <b><%= t'.first_name' %>:</b>
          <%= @iph_person.first_name %>
        </p>
              <p>
          <b><%= t'.last_name1' %>:</b>
          <%= @iph_person.last_name1 %>
        </p>
              <p>
          <b><%= t'.last_name2' %>:</b>
          <%= @iph_person.last_name2 %>
        </p>
              <p>
          <b><%= t'.alias' %>:</b>
          <%= @iph_person.alias %>
        </p>
              <p>
          <b><%= t'.birth_date' %>:</b>
          <%= @iph_person.birth_date %>
        </p>
              <p>
          <b><%= t'.originative' %>:</b>
          <%= @iph_person.originative %>
        </p>
              <p>
          <b><%= t'.genre' %>:</b>
          <%= @iph_person.genre %>
        </p>
              <p>
          <b><%= t'.record' %>:</b>
          <%= @iph_person.record %>
        </p>
              <p>
          <b><%= t'.profession' %>:</b>
          <%= @iph_person.profession %>
        </p>
              <p>
          <b><%= t'.observations' %>:</b>
          <%= @iph_person.observations %>
        </p>
              <p>
          <b><%= t'.status' %>:</b>
          <%= @iph_person.status_id %>
        </p>
          </div>
  </div>
</div>

<div class="form-actions">
  <%if can? :index, Iph Person %>
  <%= link_to t(:back, :default => t('helpers.links.back')), iph_people_path, :class => 'btn' %>
  <%end%>
  <%if can? :edit, Iph Person %>
  <%= link_to t(:edit, :default => t('helpers.links.edit')), edit_iph_person_path(@iph_person), :class => 'btn' %>
  <%end%>
  <%if can? :destroy, Iph Person %>
  <%= link_to t(:destroy, :default => t("helpers.links.destroy")), iph_person_path(@iph_person),
  :method => 'delete',
  :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
  :class => 'btn btn-danger' %>
  <%end%>
  <%if can? :new, Iph Person %>
  <%= link_to t(:new, :default => t('helpers.links.edit')), new_iph_person_path, :class => 'btn btn-primary', :title => t(:new) %>
  <%end%>
</div>